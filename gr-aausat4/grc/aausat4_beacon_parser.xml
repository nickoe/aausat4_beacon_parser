<?xml version="1.0"?>
<block>
  <name>beacon_parser</name>
  <key>aausat4_beacon_parser</key>
  <category>aausat4</category>
  <import>import aausat4</import>
  <make>aausat4.beacon_parser($packet_len)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Packet Length</name>
    <key>packet_len</key>
    <type>int</type>
  </param>
  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

</block>
